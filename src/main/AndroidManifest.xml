<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="stud.ft"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk android:minSdkVersion="19"></uses-sdk>
    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:name=".application.FitnessApp_"
        android:theme="@style/AppTheme"
        >
        <meta-data android:name="roboguice.modules" android:value="stud.ft.application.AppModule"/>
        <activity
            android:name=".MainActivity_"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-feature android:name="android.hardware.sensor.accelerometer" android:required="true"></uses-feature>

</manifest>
